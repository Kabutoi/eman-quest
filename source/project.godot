; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Eman Quest"
run/main_scene="res://Scenes/Title.tscn"
config/icon="res://icon.png"

[autoload]

StartUp="*res://Scripts/StartUp.gd"
Features="*res://Scripts/Features.gd"
Globals="*res://Scripts/Globals.gd"

[display]

window/size/width=960
window/size/height=576
window/stretch/mode="viewport"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[editor_plugins]

enabled=PoolStringArray( "gut", "paulloz.colorblindness" )

[gui]

theme/custom="res://Theme/GlobalTheme.tres"
theme/custom_font="res://Theme/Default-24px.tres"

[logging]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/EmanQuest.log"

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
